#frontend

style = Style(
	{
		backgroundColor: "#00f"
		padding: "10px"
	}
)

style2 = Style(
	{
		backgroundColor: "#0f0"
		padding: "9px"
	}
)

class Container extends Component
	function render()
		return <div
			style=style2
		>
			this.children
		</div>

app = <div
	style=style
>
	<Container>
		"foo"
	</Container>
</div>

render(
	app
)